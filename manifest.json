{
    "name": "Lotto Checker",
    "description": "Lotto checker",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [        
        "tabs", "scripting"
    ],
    "host_permissions": [
        "*://*/*",
        "<all_urls>"
    ]
}